{
  "name": "neseditor",
  "version": "0.0.1",
  "description": "NES editor for creating NES games and assets",
  "keywords": [
    "NES",
    "editor",
    "sprites",
    "music",
    "graphics"
  ],
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./app",
    "build": "webpack",
    "dev": "webpack-dev-server",
    "start": "node server",
    "compileNES": "./nesasm/bin/nesasm ./example/example.asm"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.(css|less)$": "./config/jest/css-transform.js",
      "^(?!.*\\.(js|jsx|mjs|css|less|json)$)": "./config/jest/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  },
  "author": "Eric Andre",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "null"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^22.2.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.9.0",
    "jest": "^22.3.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.8.2",
    "react-router-redux": "^4.0.7",
    "react-test-renderer": "^15.6.2",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.1",
    "redux-undo": "^0.6.1"
  }
}
